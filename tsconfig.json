{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "typeRoots": ["./node_modules/@types", "./types", "index.d.ts"]
  },
  "include": ["src/**/*.ts", "index.d.ts", "src/scripts/user.script.ts", "src/scripts/channels.script.ts"],
  "exclude": ["node_modules"]
}
